## Example Metabase/BI environment variables (copy to repo root .env and adjust)
# Postgres (BI warehouse)
PG_HOST=postgres
PG_PORT=5432
PG_DB=vita_bi
PG_USER=vita
PG_PASSWORD=vita_pwd

# Metabase application port
MB_JETTY_PORT=3000
MB_URL=http://localhost:3000

# Metabase bootstrap admin (used by setup_metabase.py)
MB_ADMIN_EMAIL=admin@example.com
MB_ADMIN_PASS=P@ssw0rd123

# Agent (FastAPI NL->SQL)
AGENT_PORT=8088

# LLM (OpenAI-compatible; Ollama or gateway)
LLM_BASE_URL=http://host.docker.internal:11434/v1
LLM_MODEL=deepseek-v3.1:671b-cloud
LLM_API_KEY=ollama
SQL_LOG_ENABLED=1
SQL_LOG_RAW=0

# Optional timezone
TZ=Asia/Shanghai
